{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "jest/globals": true 
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "no-underscore-dangle": [
            2,
            {
                "allow": [
                    "__filename",
                    "__dirname"
                ]
            }
        ]
    }
}